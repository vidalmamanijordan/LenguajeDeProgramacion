<form onsubmit="upeu.guardarAlumno('alumnos/nuevo');return false;"
	id="formulario">
	<h1>Registro de Alumno</h1>
	<p>
		<input type="text" id="nombre" placeholder="Nombre" autofocus required>
	</p>
	<p>
		<input type="text" id="paterno" placeholder="Paterno" required>
	</p>
	<p>
		<input type="text" id="materno" placeholder="Materno" required>
	</p>
	<p>
		<input type="submit" value="Guardar">
	</p>
</form>